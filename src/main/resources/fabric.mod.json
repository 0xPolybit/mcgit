{
	"schemaVersion": 1,
	"id": "mcgit",
	"version": "${version}",
	"name": "Polybit's Git Version Control",
	"description": "A Fabric mod for Git version control in Minecraft.",
	"authors": [
		"0xPolybit"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/0xPolybit/mcgit"
	},
	"license": "CC0-1.0",
	"icon": "assets/mcgit/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"tech.polybit.mcgit.PolybitsGitVersionControl"
		],
		"fabric-datagen": [
			"tech.polybit.mcgit.PolybitsGitVersionControlDataGenerator"
		],
		"client": [
			"tech.polybit.mcgit.PolybitsGitVersionControlClient"
		]
	},
	"mixins": [
		"mcgit.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	}
}